# Telegram Bot API Token (get from @BotFather in Telegram)
# Create a new bot: /start -> /newbot -> follow instructions
TELEGRAM_BOT_TOKEN=your_bot_token_here

# Super Admin ID (Telegram user ID with full system access)
# To get your ID: use @userinfobot in Telegram
# Multiple admins: separate with comma (e.g., 123456789, 987654321)
SUPER_ADMIN_ID=your_telegram_user_id
